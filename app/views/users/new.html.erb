<h1>Sign Up</h1>

<%= form_for @user do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %> <br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br><br>



  <%= f.submit "Sign Up" %>



<% end %>
